{
  "presets": ["react", "es2015", "stage-0"],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": [
        "babel-plugin-dev-expression"
      ]
    },
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          "webpack-loaders",
          { "config": "webpack.config.node.js", "verbose": false }
        ]
      ]
    }
  }
}
